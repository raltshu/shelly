{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name":"req",
      "methods": [
        "get",
        "post"
      ],
      "route": "{action:alpha}/{device_id}/{channel_id:int?}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    },
    {
      "type": "cosmosDB",
      "direction": "out",
      "name": "doc",
      "databaseName": "shellyAction",
      "collectionName": "buttonOnOff",
      "createIfNotExists": "false",
      "connectionStringSetting": "CosmosDbConnectionString"
    }
  ]
}
